{
  "name": "reversi-react",
  "version": "0.1.0",
  "description": "A Reversi game implemented in React / TypeScript",
  "repository": {
    "type": "git",
    "url": "git@github.com-pub:shuntksh/reversi-react.git"
  },
  "author": "Shun <shuntksh@gmail.com>",
  "license": "MIT",
  "scripts": {
    "compile": "rimraf ./build && cross-env NODE_ENV=production webpack",
    "check": "npm run npm-check && npm run nsp",
    "css": "tcm --pattern ./src/**/*.css --camelCase",
    "npm-check": "npm-check --skip-unused",
    "nsp": "nsp check",
    "start": "concurrently --kill-others \"npm run css -- --watch\" \"npm run dev-server\"",
    "dev-server": "cross-env NODE_ENV=development webpack-dev-server --hot --inline"
  },
  "devDependencies": {
    "@types/chai": "4.0.1",
    "@types/classnames": "2.2.0",
    "@types/mocha": "2.2.41",
    "@types/react": "15.0.38",
    "@types/react-addons-test-utils": "0.14.19",
    "@types/react-dom": "15.5.1",
    "awesome-typescript-loader": "3.2.1",
    "chai": "4.1.0",
    "classnames": "2.2.5",
    "concurrently": "3.5.0",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "cssnano": "3.10.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.29.0",
    "mkdirp": "0.5.1",
    "mocha": "3.4.2",
    "ncp": "2.0.0",
    "npm-check": "5.4.4",
    "nsp": "2.6.3",
    "postcss-cssnext": "3.0.2",
    "postcss-loader": "2.0.6",
    "postcss-reporter": "4.0.0",
    "react": "15.6.1",
    "react-addons-test-utils": "15.6.0",
    "react-dev-utils": "3.0.2",
    "react-dom": "15.6.1",
    "react-hot-loader": "1.3.1",
    "rimraf": "2.6.1",
    "style-loader": "0.18.2",
    "stylelint": "7.13.0",
    "stylelint-config-standard": "17.0.0",
    "ts-node": "3.2.0",
    "tslint": "5.5.0",
    "tslint-loader": "3.5.3",
    "tslint-react": "3.0.0",
    "typed-css-modules": "0.2.0",
    "typescript": "2.4.1",
    "url-loader": "0.5.9",
    "webpack": "3.2.0",
    "webpack-dev-server": "2.5.1"
  },
  "dependencies": {
    "t": "^0.5.1"
  }
}
