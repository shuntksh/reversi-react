{
  "name": "reversi-react",
  "version": "0.1.0",
  "description": "A Reversi game implemented in React / TypeScript",
  "repository": {
    "type": "git",
    "url": "git@github.com-pub:shuntksh/reversi-react.git"
  },
  "author": "Shun <shuntksh@gmail.com>",
  "license": "MIT",
  "scripts": {
    "compile": "rimraf ./build && cross-env NODE_ENV=production webpack",
    "check": "npm run npm-check && npm run nsp",
    "css": "tcm --pattern ./src/**/*.css --camelCase",
    "npm-check": "npm-check --skip-unused",
    "nsp": "nsp check",
    "start": "concurrently --kill-others \"npm run css -- --watch\" \"npm run dev-server\"",
    "dev-server": "cross-env NODE_ENV=development webpack-dev-server --hot --inline"
  },
  "devDependencies": {
    "@types/chai": "4.0.4",
    "@types/classnames": "2.2.3",
    "@types/mocha": "2.2.44",
    "@types/react": "16.0.21",
    "@types/react-addons-test-utils": "0.14.20",
    "@types/react-dom": "16.0.2",
    "awesome-typescript-loader": "3.3.0",
    "chai": "4.1.2",
    "concurrently": "3.5.0",
    "cross-env": "5.1.1",
    "css-loader": "0.28.7",
    "cssnano": "3.10.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "html-webpack-plugin": "2.30.1",
    "mkdirp": "0.5.1",
    "mocha": "4.0.1",
    "ncp": "2.0.0",
    "npm-check": "5.5.2",
    "nsp": "3.1.0",
    "postcss-cssnext": "3.0.2",
    "postcss-loader": "2.0.8",
    "postcss-reporter": "5.0.0",
    "react-dev-utils": "4.2.1",
    "react-hot-loader": "3.1.1",
    "rimraf": "2.6.2",
    "style-loader": "0.19.0",
    "stylelint": "8.2.0",
    "stylelint-config-standard": "17.0.0",
    "ts-node": "3.3.0",
    "tslint": "5.8.0",
    "tslint-loader": "3.5.3",
    "tslint-react": "3.2.0",
    "typed-css-modules": "0.3.1",
    "typescript": "2.6.1",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "react": "16.0.0",
    "react-dom": "16.0.0"
  }
}
